#ifndef __MBEDCRYPTO_CONFIG_H__
#define __MBEDCRYPTO_CONFIG_H__
//-----------------------------------------------------------------------------

// mbedcrypto has been built by static c/c++ runtime
#cmakedefine MBEDCRYPTO_STATIC_CRT

// message digest (hash)
#cmakedefine MBEDCRYPTO_MD2
#cmakedefine MBEDCRYPTO_MD4

// cipher modes:
// cipher feedback mode
#cmakedefine MBEDCRYPTO_CFB
// cipher counter mdoe
#cmakedefine MBEDCRYPTO_CTR
// Galois cipher mode (for AEAD)
#cmakedefine MBEDCRYPTO_GCM
// counter cbc-mac mode (for AEAD)
#cmakedefine MBEDCRYPTO_CCM

// DES and 3DES
#cmakedefine MBEDCRYPTO_DES
#cmakedefine MBEDCRYPTO_BLOWFISH
#cmakedefine MBEDCRYPTO_CAMELLIA
#cmakedefine MBEDCRYPTO_ARC4

// PEM/DER key export
#cmakedefine MBEDCRYPTO_PK_EXPORT
// rsa key generator
#cmakedefine MBEDCRYPTO_RSA_KEYGEN
// elliptic curve algorithms as: ekey, eckey_dh, ecdsa, ...
#cmakedefine MBEDCRYPTO_EC

// mbedcrypto has been built by Qt5 binding around QByteArray
#cmakedefine MBEDCRYPTO_Qt5
//-----------------------------------------------------------------------------
#endif // __MBEDCRYPTO_CONFIG_H__
