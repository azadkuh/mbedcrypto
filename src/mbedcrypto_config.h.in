#ifndef MBEDCRYPTO_CONFIG_H
#define MBEDCRYPTO_CONFIG_H
//-----------------------------------------------------------------------------

// mbedcrypto has been built by static c/c++ runtime
#cmakedefine MBEDCRYPTO_STATIC_CRT

// message digest (hash)
#cmakedefine MBEDCRYPTO_MD2
#cmakedefine MBEDCRYPTO_MD4
#cmakedefine MBEDCRYPTO_RIPEMD160

// cipher blocking modes:
#cmakedefine MBEDCRYPTO_BM_CFB
#cmakedefine MBEDCRYPTO_BM_OFB
#cmakedefine MBEDCRYPTO_BM_CTR
#cmakedefine MBEDCRYPTO_BM_XTS
#cmakedefine MBEDCRYPTO_BM_GCM
#cmakedefine MBEDCRYPTO_BM_CCM

// cipher algorithms
#cmakedefine MBEDCRYPTO_ARIA
#cmakedefine MBEDCRYPTO_BLOWFISH
#cmakedefine MBEDCRYPTO_CAMELLIA
#cmakedefine MBEDCRYPTO_CHACHA20
#cmakedefine MBEDCRYPTO_DES
#cmakedefine MBEDCRYPTO_ARC4

// rsa key generator
#cmakedefine MBEDCRYPTO_RSA_KEYGEN
// elliptic curve algorithms as: ekey, eckey_dh, ecdsa, ...
#cmakedefine MBEDCRYPTO_EC

//-----------------------------------------------------------------------------
#endif // MBEDCRYPTO_CONFIG_H
