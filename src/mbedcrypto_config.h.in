#ifndef MBEDCRYPTO_CONFIG_H
#define MBEDCRYPTO_CONFIG_H
//-----------------------------------------------------------------------------

// message digest (hash)
#cmakedefine MBEDCRYPTO_HASH_MD2
#cmakedefine MBEDCRYPTO_HASH_MD4
#cmakedefine MBEDCRYPTO_HASH_RIPEMD160

// cipher blocking modes:
#cmakedefine MBEDCRYPTO_BM_CFB
#cmakedefine MBEDCRYPTO_BM_OFB
#cmakedefine MBEDCRYPTO_BM_CTR
#cmakedefine MBEDCRYPTO_BM_XTS
#cmakedefine MBEDCRYPTO_BM_GCM
#cmakedefine MBEDCRYPTO_BM_CCM

// cipher algorithms
#cmakedefine MBEDCRYPTO_CIPHER_ARIA
#cmakedefine MBEDCRYPTO_CIPHER_BLOWFISH
#cmakedefine MBEDCRYPTO_CIPHER_CAMELLIA
#cmakedefine MBEDCRYPTO_CIPHER_CHACHA20
#cmakedefine MBEDCRYPTO_CIPHER_DES
#cmakedefine MBEDCRYPTO_CIPHER_ARC4

// private/public key generator
#cmakedefine MBEDCRYPTO_PK_KEYGEN
// elliptic curve algorithms as: ekey, eckey_dh, ecdsa, ...
#cmakedefine MBEDCRYPTO_PK_EC

//-----------------------------------------------------------------------------
#endif // MBEDCRYPTO_CONFIG_H
